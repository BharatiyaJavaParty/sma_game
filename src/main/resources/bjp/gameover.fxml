<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="432.0" prefWidth="471.0" spacing="20" stylesheets="@../styles/gameOver.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bjp.controller.GameOverController">
   <children>
      <Label fx:id="message" contentDisplay="CENTER" styleClass="game-label" text="GAME OVER"/>
      <Button fx:id="retryButton" onAction="#retryButtonClicked" prefHeight="60.0" prefWidth="300.0" text="RETRY" styleClass="button"/>
      <Button fx:id="scoreButton" onAction="#scoreButtonClicked" prefHeight="60.0" prefWidth="300.0" text="SCORE" styleClass="button"/>
   </children>
</VBox>

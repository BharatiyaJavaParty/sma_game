<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" prefHeight="720" prefWidth="1280" fx:controller="bjp.controller.ScoreBoardController" stylesheets="@../styles/scoreboard.css">
    <ScrollPane>
        <content>
            <TableView fx:id="scoreboardTable" prefHeight="720" prefWidth="1280">
                <columns>
                    <TableColumn fx:id="name" text="Player Name" prefWidth="256"/>
                    <TableColumn fx:id="co2Budget" text="CO2 spent" prefWidth="256"/>
                    <TableColumn fx:id="time" text="Player Time" prefWidth="256"/>
                    <TableColumn fx:id="gems" text="Gems" prefWidth="256"/>
                    <TableColumn fx:id="TimeStamp" text="TimeStamp" prefWidth="256"/>
                </columns>
            </TableView>
        </content>
    </ScrollPane>

</VBox>